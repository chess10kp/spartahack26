{
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "media-devices",
      "permissions": [
        "allow-enumerate-media-devices",
        "allow-open-media-devices"
      ]
    },
    "shell:allow-open"
  ]
}
